{
  "name": "react-github-calendar",
  "version": "0.16.0",
  "description": " A flexible React component to display a GitHub contributions graph",
  "author": "Jonathan Gruber <gruberjonathan@gmail.com>",
  "license": "MIT",
  "homepage": "https://grubersjoe.github.io/react-github-calendar/",
  "repository": "grubersjoe/react-github-calendar",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rollup -c",
    "deploy": "gh-pages -d example/build",
    "format": "prettier --write './*' 'src/**/*' 'example/**/*'",
    "lint": "eslint 'src/**/*' 'example/**/*'",
    "predeploy": "cd example && yarn install && yarn run build",
    "prepare": "yarn run build",
    "start": "rollup -c -w",
    "test:watch": "react-scripts test --env=jsdom",
    "test": "cross-env CI=1 react-scripts test --env=jsdom"
  },
  "dependencies": {
    "date-fns": "^1.30.0",
    "tinycolor2": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^3.3.0",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-url": "^3.0.1"
  },
  "peerDependencies": {
    "prop-types": "^15.7.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-tooltip": "^3.9.0"
  },
  "files": [
    "dist"
  ]
}
